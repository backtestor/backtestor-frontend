---
import SignoutForm from "@src/components/auth/SignoutForm.astro";
import LogoLayout from "@src/layouts/LogoLayout.astro";
import { changeLanguage, t } from "i18next";

changeLanguage("en");

const title = t("pages.signin.title");
const description = t("pages.signin.description");
---

<LogoLayout title={title} description={description} noindex={true} nofollow={true} requiresAuth={true} jsonLd={null}>
  <SignoutForm />
</LogoLayout>
