---
import AuthResponse from "@src/components/auth/AuthResponse.astro";
import LogoLayout from "@src/layouts/LogoLayout.astro";
import { changeLanguage, t } from "i18next";

changeLanguage("en");

const title = t("pages.auth-response.title");
const description = t("pages.auth-response.description");
---

<LogoLayout title={title} description={description} noindex={true} nofollow={true} requiresAuth={false} jsonLd={null}>
  <AuthResponse />
</LogoLayout>;
