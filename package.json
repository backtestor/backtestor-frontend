  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "astro dev --verbose",
    "build": "npx astro-i18next generate && astro build",
    "preview": "astro preview --verbose",
    "start": "node ./dist/server/entry.mjs",
    "lint": "astro check && tsc --noEmit",
